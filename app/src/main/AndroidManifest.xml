<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.RxCompare">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_logo"
        android:roundIcon="@mipmap/ic_launcher_logo"
        android:screenOrientation="portrait"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.RxCompare">
        <activity android:name="com.example.RxCompare.SearchItemActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.example.RxCompare.ShoppingSessionActivity"
            android:screenOrientation="portrait">

        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name="com.example.RxCompare.ShoppingSessionsListActivity" />
        </application>

</manifest>